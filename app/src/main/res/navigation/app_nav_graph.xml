<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/app_nav_graf"
            app:startDestination="@id/dashBoardFragment">

    <fragment
            android:id="@+id/dashBoardFragment"
            android:name="com.nimtego.armaplc.DashBoardFragment"
            android:label="fragment_dash_board"
            tools:layout="@layout/fragment_dash_board">
        <action
                android:id="@+id/action_dashBoardFragment_to_stationListFragment2"
                app:destination="@id/stationListFragment2"/>
        <action android:id="@+id/action_dashBoardFragment_to_addStationFragment"
                app:destination="@id/addStationFragment"/>
    </fragment>

    <fragment
            android:id="@+id/stationFragment"
            android:name="com.nimtego.armaplc.StationFragment"
            android:label="fragment_station"
            tools:layout="@layout/fragment_station"/>
    <fragment
            android:id="@+id/stationListFragment2"
            android:name="com.nimtego.armaplc.StationListFragment"
            android:label="fragment_station_list"
            tools:layout="@layout/fragment_station_list">
        <action
                android:id="@+id/action_stationListFragment2_to_stationFragment"
                app:destination="@id/stationFragment"/>
        <action android:id="@+id/action_stationListFragment2_to_addStationFragment"
                app:destination="@id/addStationFragment"/>
    </fragment>
    <fragment android:id="@+id/settingsFragment"
              android:name="com.nimtego.armaplc.SettingsFragment"
              android:label="fragment_settings"
              tools:layout="@layout/fragment_settings"/>
    <fragment android:id="@+id/addStationFragment" android:name="com.nimtego.armaplc.AddStationFragment"
              android:label="fragment_add_station" tools:layout="@layout/fragment_add_station"/>
    <activity android:id="@+id/appActivity2" android:name="com.nimtego.armaplc.AppActivity"
              android:label="activity_main" tools:layout="@layout/activity_main"/>
    <fragment android:id="@+id/bottonFragment" android:name="com.nimtego.armaplc.BottonFragment"
              android:label="fragment_botton" tools:layout="@layout/fragment_botton">
        <action android:id="@+id/action_bottonFragment_to_dashBoardFragment" app:destination="@id/dashBoardFragment"/>
        <action android:id="@+id/action_bottonFragment_to_settingsFragment" app:destination="@id/settingsFragment"/>
        <action android:id="@+id/action_bottonFragment_to_stationListFragment2"
                app:destination="@id/stationListFragment2"/>
    </fragment>
</navigation>